--TEST--
Twig supports the in operator when using objects
--TEMPLATE--
{% if object in object_list %}
TRUE
{% endif %}
--DATA--
$foo = new TwigTestFoo();
$foo1 = new TwigTestFoo();

$foo->position = $foo1;
$foo1->position = $foo;

return array(
    'object'      => $foo,
    'object_list' => array($foo1, $foo),
);
--EXPECT--
TRUE
