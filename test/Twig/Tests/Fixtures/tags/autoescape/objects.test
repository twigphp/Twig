--TEST--
"autoescape" tag applies escaping to object method calls
--TEMPLATE--
{% autoescape 'html' %}
{{ user.name }}
{{ user.name|lower }}
{{ user }}
{% endautoescape %}
--DATA--
class UserForAutoEscapeTest
{
  public function getName()
  {
    return 'Fabien<br />';
  }

  public function __toString()
  {
     return 'Fabien<br />';
  }
}
return array('user' => new UserForAutoEscapeTest())
--EXPECT--
Fabien&lt;br /&gt;
fabien&lt;br /&gt;
Fabien&lt;br /&gt;
